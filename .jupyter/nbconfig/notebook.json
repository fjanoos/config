{
  "load_extensions": {
    "rubberband/main": false,
    "contrib_nbextensions_help_item/main": true,
    "select_keymap/main": true,
    "keyboard_shortcut_editor/main": true,
    "nbextensions_configurator/config_menu/main": true,
    "codefolding/main": true,
    "toggle_all_line_numbers/main": true,
    "runtools/main": true,
    "code_font_size/code_font_size": false,
    "code_prettify/code_prettify": true
  },
  "kse_show_rebinds": true,
  "stored_keymap": "sublime",
  "kse_rebinds": {
    "edit": [
      {
        "action_name": "jupyter-notebook:restart-kernel-and-clear-output",
        "to": "f2"
      }
    ],
    "command": [
      {
        "action_name": "jupyter-notebook:restart-kernel-and-clear-output",
        "to": "f2"
      }
    ]
  },
  "keys": {
    "command": {
      "bind": {
        "shift-enter": "jupyter-notebook:run-cell-and-select-next",
        "alt-enter": "jupyter-notebook:run-cell",
        "ctrl-enter": "jupyter-notebook:run-cell",
        "f3": "jupyter-notebook:confirm-restart-kernel-and-clear-output",
        "f2": "jupyter-notebook:restart-kernel"
      },
      "unbind": [
        "alt-enter",
        "ctrl-enter"
      ]
    }
  },
  "Cell": {
    "cm_config": {
      "lineNumbers": true
    }
  }
}